{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "fileName": false,
        "displayName": true
      }
    ],
    ["inline-react-svg"],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@root": "./",
          "@components": "./components"
        }
      }
    ]
  ]
}
